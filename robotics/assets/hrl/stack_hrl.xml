<mujoco>
	<compiler angle="radian" coordinate="local" meshdir="../stls/fetch" texturedir="../textures" />
	<option timestep="0.002">
		<flag warmstart="enable" />
	</option>
	<size nconmax="5000" njmax="5000" />

	<include file="shared.xml" />

	<worldbody>
