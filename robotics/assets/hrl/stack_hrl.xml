		<body name="floor2" pos="29 29 30">
			<site name="target_goal" pos="0 0 0.5" size="0.025 0.025 0.025" rgba="0 1 1 0.32" type="sphere" />
		</body>

		<body name="floor3" pos="29 29 31">
